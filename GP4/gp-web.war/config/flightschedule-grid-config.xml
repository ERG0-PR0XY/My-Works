			<filter>true</filter>
			<search>true</search>
			<resize>true</resize>
			<dragable>true</dragable>
		</column>

		--><column>
			<title>STD(HHMM)</title>
			<dataField>std</dataField>
			<dataType>String</dataType>
			<width>180</width>
			<visible>true</visible>
