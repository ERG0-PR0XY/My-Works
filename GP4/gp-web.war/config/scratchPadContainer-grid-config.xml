<?xml version="1.0" encoding="UTF-8"?>
<GridConfig>
	<id>Container Scratch Pad</id>
	<name>Container Scratch Pad</name>
	<size>
		<height>400</height>
		<width>700</width>
	</size>	
	<pagination>		
		<loadingSize>100</loadingSize>
	</pagination>
	<header>
		<show>true</show>
		<title>Custom Grid Component</title>
		<align>left</align>
	</header>
	<views>
		<show>true</show>
		<title>Views</title>
		<defaultViewText>Default</defaultViewText>		
	</views>
	<search>
		<show>true</show>
		<title>Search</title>
		<textWidth>300</textWidth>		
	</search>      
    <hideShowColumns>
		<show>true</show>
		<title>Show/ Hide Columns</title>		
    </hideShowColumns>  
	<lookup>
		<show>true</show>
		<title>Quick Look</title>
		<textWidth>200</textWidth>		
	</lookup>      
	<recordCount>
		<show>true</show>
		<title>Containers :</title>		
	</recordCount>
    <tools>
    	<title>Tools</title>
		<show>true</show>		
    </tools>
    <contextMenu>
    	<menus>
    		<menu>
    			<title>Add Task</title>
    			<icon>images/add.png</icon>
    			<function>doAdd</function>
    			<type>grid</type>
    		</menu>
    		<menu>
    			<title>Edit Task</title>
    			<icon>images/edit.png</icon>
    			<function>doEdit</function>
    			<type>row</type>
    		</menu>
    		<menu>
    			<title>Delete Task</title>
    			<icon>images/delete.png</icon>
    			<function>doDelete</function>
    			<type>row</type>
    		</menu>
    	</menus>
    </contextMenu>
    <functions>
    	<rowClickCallback>callSingleClick</rowClickCallback>
    	<rowDoubleClickCallback>callDoubleClick</rowDoubleClickCallback>
    </functions>

