<?xml version="1.0" encoding="UTF-8"?>
<GridConfig>
	<id>Generated Reports</id>
	<name>Generated Reports</name>
	<size>
		<height>400</height>
		<width>700</width>
	</size>	
	<pagination>		
		<loadingSize>100</loadingSize>
	</pagination>
	<header>
		<show>true</show>
		<title>Custom Grid Component</title>
		<align>left</align>
	</header>
	<views>
		<show>true</show>
		<title>Views</title>
		<defaultViewText>Default</defaultViewText>		
	</views>
	<search>
		<show>false</show>
		<title>Search</title>
		<textWidth>300</textWidth>		
	</search>      
    <hideShowColumns>
		<show>true</show>
		<title>Show/ Hide Columns</title>		
    </hideShowColumns>  
	<lookup>
		<show>true</show>
		<title>Quick Look</title>
		<textWidth>200</textWidth>		
	</lookup>      
	<recordCount>
		<show>true</show>
		<title>Reports :</title>		
	</recordCount>
    <tools>
    	<title>Tools</title>
		<show>true</show>		
    </tools>
    <contextMenu>
      <menus>
         <menu>
            <title>Add Task</title>
            <icon>images/add.png</icon>
            <function>doAdd</function>
            <type>grid</type>
         </menu>
         <menu>
            <title>Edit Task</title>
            <icon>images/edit.png</icon>
            <function>doEdit</function>
            <type>row</type>
         </menu>
         <menu>
            <title>Delete Task</title>
            <icon>images/delete.png</icon>
            <function>doDelete</function>
            <type>row</type>
         </menu>
      </menus>
    </contextMenu>
    <functions>
      <rowClickCallback>callSingleClick</rowClickCallback>
      <rowDoubleClickCallback>callDoubleClick</rowDoubleClickCallback>
    </functions>

	<columns>
				
	    <column>
			<title>Code</title>
			<dataField>reportCode</dataField>
			<dataType>String</dataType>
			<width>160</width>
			<visible>true</visible>
			<hide>false</hide>
			<sort>true</sort>
			<filter>true</filter>
			<search>true</search>
			<resize>true</resize>
			<dragable>true</dragable>
		</column>
		<column>
         <title>Entities</title>
         <dataField>entitiesDesc</dataField>
         <dataType>String</dataType>
         <width>240</width>
         <visible>true</visible>
         <hide>false</hide>
         <sort>true</sort>
         <filter>true</filter>
         <search>true</search>
         <resize>true</resize>
         <dragable>true</dragable>
      </column>   
      <column>
         <title>Expires Date</title>
         <dataField>expiresDate</dataField>
         <dataType>Date</dataType>
         <width>Auto</width>
         <visible>true</visible>
         <hide>false</hide>
         <sort>true</sort>
         <filter>true</filter>
         <search>true</search>
         <resize>true</resize>
         <dragable>true</dragable>
      </column>   
		<column>
         <title>Status</title>
         <dataField>genStatus</dataField>
         <dataType>String</dataType>
         <width>100</width>
         <visible>true</visible>
         <hide>false</hide>
         <sort>true</sort>
         <filter>true</filter>
         <search>true</search>
         <resize>true</resize>
         <dragable>true</dragable>
      </column>   
      <column>
         <title>Output Type</title>
         <dataField>reportOutputType</dataField>
         <dataType>String</dataType>
         <width>Auto</width>
         <visible>true</visible>
         <hide>false</hide>
         <sort>true</sort>
         <filter>true</filter>
         <search>true</search>
         <resize>true</resize>
         <dragable>true</dragable>
      </column>		
		<column>
			<title>Queued Date</title>
			<dataField>queuedDateTime</dataField>
			<dataType>Date</dataType>
			<resultRenderer type="DateTimeRenderer">
            <DateTimeRenderer correctTime="true"/>
         </resultRenderer>
			<width>170</width>
			<visible>true</visible>
			<hide>false</hide>
			<sort>true</sort>
			<filter>true</filter>
			<search>true</search>
			<resize>true</resize>
			<dragable>true</dragable>
		</column>       
      <column>
         <title>Started Date</title>
         <dataField>startDateTime</dataField>
         <dataType>Date</dataType>
         <resultRenderer type="DateTimeRenderer">
            <DateTimeRenderer correctTime="true"/>
         </resultRenderer>
         <width>170</width>
         <visible>true</visible>
         <hide>false</hide>
         <sort>true</sort>
         <filter>true</filter>
         <search>true</search>
         <resize>true</resize>
         <dragable>true</dragable>
      </column>         
      <column>
         <title>Finished Date</title>
         <dataField>finishedDateTime</dataField>
         <dataType>Date</dataType>
         <resultRenderer type="DateTimeRenderer">
            <DateTimeRenderer correctTime="true"/>
         </resultRenderer>
         <width>170</width>
         <visible>true</visible>
         <hide>false</hide>
         <sort>true</sort>
         <filter>false</filter>
         <search>false</search>
         <resize>true</resize>
         <dragable>true</dragable>
      </column>			
	</columns>
</GridConfig>
