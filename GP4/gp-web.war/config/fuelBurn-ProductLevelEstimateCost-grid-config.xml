<?xml version="1.0" encoding="UTF-8"?>
<GridConfig>
	<id>G8004</id>
	<name>FuelBurn Product Level Estimate Cost</name>
	<size>
		<height>400</height>
		<width>700</width>
	</size>
	<pagination>
		<loadingSize>100</loadingSize>
	</pagination>
	<header>
		<show>true</show>
		<title>Custom Grid Component</title>
		<align>left</align>
	</header>
	<views>
		<show>false</show>
		<title>Views</title>
		<defaultViewText>Default</defaultViewText>
	</views>
	<search>
		<show>true</show>
		<title>Search</title>
		<textWidth>300</textWidth>
	</search>
	<hideShowColumns>
		<show>true</show>
		<title>Show/ Hide Columns</title>
	</hideShowColumns>
	<lookup>
		<show>false</show>
		<title>Quick Look</title>
		<textWidth>200</textWidth>
	</lookup>
	<recordCount>
		<show>true</show>
		<title>Comments</title>
	</recordCount>
	<tools>
		<title>Tools</title>
		<show>false</show>
    </tools>
    <contextMenu>
    	<menus>
    		<menu>
    			<title>Add Fuel Cost</title>
    			<icon>images/add.png</icon>
    			<function>doAdd</function>
    			<type>grid</type>
    		</menu>
    		<menu>
    			<title>Edit Fuel Cost</title>
    			<icon>images/edit.png</icon>
    			<function>doEdit</function>
    			<type>row</type>
    		</menu>
    		<menu>
    			<title>Delete Fuel Cost</title>
    			<icon>images/delete.png</icon>
    			<function>doDelete</function>
    			<type>row</type>
    		</menu>
    	</menus>
    </contextMenu>
    <functions>
    	<rowClickCallback>callSingleClick</rowClickCallback>
    	<rowDoubleClickCallback>callDoubleClick</rowDoubleClickCallback>
    </functions>

